{
    "name": "Test Extension",
    "description": "just a test",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "worker.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon1.png",
            "32": "icons/icon1.png",
            "128": "icons/icon1.png"
        }
    },
    "icons": {
        "16": "icons/icon1.png",
        "32": "icons/icon1.png",
        "128": "icons/icon1.png"
    },
    "permissions": [
        "storage", 
        "activeTab", 
        "tabs", 
        "scripting"
    ],
    "host_permissions": ["https://sentosa-jm.web.app/*"]
}